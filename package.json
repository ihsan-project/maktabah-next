{
  "name": "maktabah",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "rm -rf .next out && rm -rf app/api",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "npm run prebuild && next build && firebase deploy",
    "deploy:hosting": "npm run prebuild && next build && firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "tailwind:watch": "tailwindcss -i ./app/globals.css -o ./app/output.css --watch"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.10.0",
    "firebase": "^10.6.0",
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  }
}